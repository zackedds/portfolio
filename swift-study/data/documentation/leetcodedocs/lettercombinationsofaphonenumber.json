{"abstract":[{"text":"LeetCode #17 • Medium • Backtracking (BFS)","type":"text"}],"primaryContentSections":[{"declarations":[{"platforms":["macOS"],"languages":["swift"],"tokens":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"LetterCombinationsOfAPhoneNumber","kind":"identifier"}]}],"kind":"declarations"},{"content":[{"level":2,"text":"Overview","anchor":"overview","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Letter Combinations of a Phone Number"}]},{"text":"Intuition","type":"heading","anchor":"Intuition","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"We map digits to letters using a Dictionary. We can solve this iteratively (BFS) by maintaining a list of ‘possible words’. For each new digit, we remove existing words, append the new possible letters to them, and push them back into the queue."}]},{"type":"thematicBreak"},{"text":"Complexity Analysis","type":"heading","anchor":"Complexity-Analysis","level":2},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"Time Complexity"}],"type":"strong"},{"type":"text","text":": "},{"type":"codeVoice","code":"O(4^N)"}]}]},{"content":[{"inlineContent":[{"inlineContent":[{"type":"text","text":"Space Complexity"}],"type":"strong"},{"text":": ","type":"text"},{"type":"codeVoice","code":"O(4^N)"}],"type":"paragraph"}]}]},{"type":"thematicBreak"},{"text":"Solution","type":"heading","anchor":"Solution","level":2},{"type":"codeListing","syntax":"swift","code":["class Solution {","    func letterCombinations(_ digits: String) -> [String] {","        guard !digits.isEmpty else { return [] }","        let numToLetter: [Character: [String]] = [","            \"2\": [\"a\", \"b\", \"c\"], \"3\": [\"d\", \"e\", \"f\"],","            \"4\": [\"g\", \"h\", \"i\"], \"5\": [\"j\", \"k\", \"l\"],","            \"6\": [\"m\", \"n\", \"o\"], \"7\": [\"p\", \"q\", \"r\", \"s\"],","            \"8\": [\"t\", \"u\", \"v\"], \"9\": [\"w\", \"x\", \"y\", \"z\"]","        ]","","        var result: [String] = [\"\"]","        ","        for digit in digits {","            guard let letters = numToLetter[digit] else { continue }","            var temp: [String] = []","            for existing in result {","                for char in letters {","                    temp.append(existing + char)","                }","            }","            result = temp","        }","        return result","    }","}"]}],"kind":"content"}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/LetterCombinationsOfAPhoneNumber"},"sections":[],"kind":"symbol","variants":[{"paths":["\/documentation\/leetcodedocs\/lettercombinationsofaphonenumber"],"traits":[{"interfaceLanguage":"swift"}]}],"hierarchy":{"paths":[["doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs"]]},"schemaVersion":{"minor":3,"patch":0,"major":0},"metadata":{"symbolKind":"struct","modules":[{"name":"LeetCodeDocs"}],"fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"LetterCombinationsOfAPhoneNumber"}],"externalID":"s:12LeetCodeDocs32LetterCombinationsOfAPhoneNumberV","roleHeading":"Structure","title":"LetterCombinationsOfAPhoneNumber","role":"symbol","navigatorTitle":[{"text":"LetterCombinationsOfAPhoneNumber","kind":"identifier"}],"color":{"standardColorIdentifier":"orange"}},"references":{"doc://LeetCodeDocs/documentation/LeetCodeDocs/LetterCombinationsOfAPhoneNumber":{"fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"LetterCombinationsOfAPhoneNumber"}],"identifier":"doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/LetterCombinationsOfAPhoneNumber","abstract":[{"text":"LeetCode #17 • Medium • Backtracking (BFS)","type":"text"}],"type":"topic","title":"LetterCombinationsOfAPhoneNumber","url":"\/documentation\/leetcodedocs\/lettercombinationsofaphonenumber","rol
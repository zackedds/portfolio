{"kind":"symbol","hierarchy":{"paths":[["doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs"]]},"schemaVersion":{"major":0,"patch":0,"minor":3},"abstract":[{"text":"LeetCode #2 • Medium • Linked Lists","type":"text"}],"topicSections":[{"generated":true,"title":"Classes","identifiers":["doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/AddTwoNumbers\/ListNode"],"anchor":"Classes"},{"generated":true,"title":"Type Methods","identifiers":["doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/AddTwoNumbers\/solve(_:_:)"],"anchor":"Type-Methods"}],"metadata":{"navigatorTitle":[{"kind":"identifier","text":"AddTwoNumbers"}],"title":"AddTwoNumbers","role":"symbol","color":{"standardColorIdentifier":"orange"},"symbolKind":"struct","externalID":"s:12LeetCodeDocs13AddTwoNumbersV","roleHeading":"Structure","modules":[{"name":"LeetCodeDocs"}],"fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"text":"AddTwoNumbers","kind":"identifier"}]},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["macOS"],"tokens":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"AddTwoNumbers","kind":"identifier"}],"languages":["swift"]}]},{"content":[{"anchor":"overview","level":2,"text":"Overview","type":"heading"},{"inlineContent":[{"type":"text","text":"Add Two Numbers"}],"type":"paragraph"},{"type":"heading","text":"Intuition","level":2,"anchor":"Intuition"},{"inlineContent":[{"text":"We iterate through both Linked Lists simultaneously, simulating manual addition by maintaining a ","type":"text"},{"code":"carry","type":"codeVoice"},{"text":" variable. We must handle cases where lists are of unequal length or a final carry remains after the last node.","type":"text"}],"type":"paragraph"},{"type":"thematicBreak"},{"anchor":"Complexity-Analysis","level":2,"text":"Complexity Analysis","type":"heading"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Time Complexity"}]},{"type":"text","text":": "},{"type":"codeVoice","code":"O(max(m, n))"}]}]},{"content":[{"inlineContent":[{"type":"strong","inlineContent":[{"text":"Space Complexity","type":"text"}]},{"text":": ","type":"text"},{"type":"codeVoice","code":"O(max(m, n))"}],"type":"paragraph"}]}],"type":"unorderedList"},{"type":"thematicBreak"},{"type":"heading","level":2,"anchor":"Solution","text":"Solution"},{"syntax":"swift","code":["\/\/ Definition for singly-linked list.","public class ListNode {","    public var val: Int","    public var next: ListNode?","    public init() { self.val = 0; self.next = nil; }","    public init(_ val: Int) { self.val = val; self.next = nil; }","    public init(_ val: Int, _ next: ListNode?) { self.val = val; self.next = next; }","}","","public static func solve(_ l1: ListNode?, _ l2: ListNode?) -> ListNode? {","    var list1: ListNode? = l1","    var list2: ListNode? = l2","","    var current: ListNode? = ListNode()","    let head: ListNode? = current","","    var carry: Int = 0","","    while list1 != nil || list2 != nil || carry != 0 {","        let val1 = list1?.val ?? 0","        let val2 = list2?.val ?? 0","","        let totalSum = val1 + val2 + carry","        let currentSum = totalSum % 10","        carry = totalSum \/ 10","","        current?.next = ListNode(currentSum)","        current = current?.next","","        list1 = list1?.next","        list2 = list2?.next","    }","","    return head?.next","}"],"type":"codeListing"}],"kind":"content"}],"sections":[],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/leetcodedocs\/addtwonumbers"]}],"identifier":{"url":"doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/AddTwoNumbers","interfaceLanguage":"swift"},"references":{"doc://LeetCodeDocs/documentation/LeetCodeDocs":{"identifier":"doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs","kind":"symbol","url":"\/documentation\/leetcodedocs","title":"LeetCodeDocs","abstract":[{"text":"My collection of Swift soluti
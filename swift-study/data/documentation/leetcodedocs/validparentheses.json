ype":"unorderedList","items":[{"content":[{"inlineContent":[{"inlineContent":[{"text":"Time Complexity","type":"text"}],"type":"strong"},{"type":"text","text":": "},{"code":"O(N)","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Space Complexity"}]},{"text":": ","type":"text"},{"code":"O(N)","type":"codeVoice"}],"type":"paragraph"}]}]},{"type":"thematicBreak"},{"text":"Solution","anchor":"Solution","type":"heading","level":2},{"code":["class Solution {","    func isValid(_ s: String) -> Bool {","        let map: [Character: Character] = [\"}\": \"{\", \"]\": \"[\", \")\": \"(\"]","        var stack: [Character] = []","","        for char in s {","            if let open = map[char] {","                \/\/ It's a closing bracket, check the stack","                if stack.popLast() != open { return false }","            } else {","                \/\/ It's an opening bracket","                stack.append(char)","            }","        }","        return stack.isEmpty","    }","}"],"syntax":"swift","type":"codeListing"}]}],"references":{"doc://LeetCodeDocs/documentation/LeetCodeDocs/MinStack":{"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"MinStack","kind":"identifier"}],"navigatorTitle":[{"text":"MinStack","kind":"identifier"}],"url":"\/documentation\/leetcodedocs\/minstack","title":"MinStack","abstract":[{"text":"LeetCode #155 • Medium • Stack","type":"text"}],"type":"topic","identifier":"doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/MinStack","kind":"symbol","role":"symbol"},"doc://LeetCodeDocs/documentation/LeetCodeDocs":{"title":"LeetCodeDocs","type":"topic","identifier":"doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs","url":"\/documentation\/leetcodedocs","role":"collection","kind":"symbol","abstract":[{"type":"text","text":"My collection of Swift solutions for LeetCode problems, focusing on optimization, time complexity, and language 
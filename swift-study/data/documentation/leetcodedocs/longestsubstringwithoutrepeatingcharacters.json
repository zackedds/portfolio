{"sections":[],"metadata":{"role":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"text":"LongestSubstringWithoutRepeatingCharacters","kind":"identifier"}],"modules":[{"name":"LeetCodeDocs"}],"navigatorTitle":[{"kind":"identifier","text":"LongestSubstringWithoutRepeatingCharacters"}],"externalID":"s:12LeetCodeDocs42LongestSubstringWithoutRepeatingCharactersV","color":{"standardColorIdentifier":"orange"},"title":"LongestSubstringWithoutRepeatingCharacters","roleHeading":"Structure","symbolKind":"struct"},"hierarchy":{"paths":[["doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs"]]},"schemaVersion":{"minor":3,"patch":0,"major":0},"identifier":{"url":"doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/LongestSubstringWithoutRepeatingCharacters","interfaceLanguage":"swift"},"abstract":[{"text":"LeetCode #3 • Medium • Sliding Window","type":"text"}],"kind":"symbol","topicSections":[{"title":"Type Methods","generated":true,"anchor":"Type-Methods","identifiers":["doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/LongestSubstringWithoutRepeatingCharacters\/solve(_:)"]}],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["macOS"],"languages":["swift"],"tokens":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"text":"LongestSubstringWithoutRepeatingCharacters","kind":"identifier"}]}]},{"content":[{"level":2,"text":"Overview","anchor":"overview","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"Longest Substring Without Repeating Characters","type":"text"}]},{"type":"heading","anchor":"Intuition","level":2,"text":"Intuition"},{"type":"paragraph","inlineContent":[{"text":"We use a ","type":"text"},{"inlineContent":[{"text":"Sliding Window","type":"text"}],"type":"strong"},{"text":" technique with ","type":"text"},{"type":"codeVoice","code":"left"},{"type":"text","text":" and "},{"code":"right","type":"codeVoice"},{"text":" pointers. We expand ","type":"text"},{"code":"right","type":"codeVoice"},{"text":" to include characters, but if we encounter a duplicate (checked via a Set), we increment ","type":"text"},{"code":"left","type":"codeVoice"},{"type":"text","text":" and remove characters until the window is valid again."}]},{"type":"thematicBreak"},{"anchor":"Complexity-Analysis","type":"heading","level":2,"text":"Complexity Analysis"},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"inlineContent":[{"text":"Time Complexity","type":"text"}],"type":"strong"},{"text":": ","type":"text"},{"code":"O(n)","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"inlineContent":[{"text":"Space Complexity","type":"text"}],"type":"strong"},{"text":": ","type":"text"},{"code":"O(k) where k is the size of the charset","type":"codeVoice"}],"type":"paragraph"}]}]},{"type":"thematicBreak"},{"anchor":"Solution","type":"heading","level":2,"text":"Solution"},{"syntax":"swift","type":"codeListing","code":["public static func solve(_ s: String) -> Int {","    let sArray: [Character] = Array(s)","    var maxSubstring = 0","    var left = 0","    var window: Set<Character> = Set()","    for (right, char) in s.enumerated() {","        if window.contains(char) {","            while window.contains(char) {","                let leftChar: Character = sArray[left]","                window.remove(leftChar)","                left += 1","            }","        }","        window.insert(char)","        maxSubstring = max(maxSubstring, right-left+1)","    }","    return maxSubstring","}"]}],"kind":"content"}],"variants":[{"paths":["\/documentation\/leetcodedocs\/longestsubstringwithoutrepeatingcharacters"],"traits":[{"interfaceLanguage":"swift"}]}],"references":{"doc://LeetCodeDocs/documentation/LeetCodeDocs/LongestSubstringWithoutRepeatingCharacters":{"role":"symbol","fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"text":"LongestSubstringWithoutRepeatingCharacters","kind":"identifier"}],"kind":"symbol","url":"\/doc
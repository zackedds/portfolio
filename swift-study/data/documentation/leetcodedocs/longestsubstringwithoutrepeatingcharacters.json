"left"},{"type":"text","text":" and remove characters until the window is valid again."}]},{"type":"thematicBreak"},{"text":"Complexity Analysis","type":"heading","level":2,"anchor":"Complexity-Analysis"},{"items":[{"content":[{"inlineContent":[{"inlineContent":[{"type":"text","text":"Time Complexity"}],"type":"strong"},{"type":"text","text":": "},{"type":"codeVoice","code":"O(n)"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"strong","inlineContent":[{"text":"Space Complexity","type":"text"}]},{"text":": ","type":"text"},{"code":"O(k) where k is the size of the charset","type":"codeVoice"}],"type":"paragraph"}]}],"type":"unorderedList"},{"type":"thematicBreak"},{"level":2,"type":"heading","text":"Solution","anchor":"Solution"},{"type":"codeListing","syntax":"swift","code":["public static func solve(_ s: String) -> Int {","    let sArray: [Character] = Array(s)","    var maxSubstring = 0","    var left = 0","    var window: Set<Character> = Set()","    for (right, char) in s.enumerated() {","        if window.contains(char) {","            while window.contains(char) {","                let leftChar: Character = sArray[left]","                window.remove(leftChar)","                left += 1","            }","        }","        window.insert(char)","        maxSubstring = max(maxSubstring, right-left+1)","    }","    return maxSubstring","}"]}]}],"kind":"symbol","hierarchy":{"paths":[["doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs"]]},"abstract":[{"text":"LeetCode #3 • Medium • Sliding Window","type":"text"}],"variants":[{"paths":["\/documentation\/leetcodedocs\/longestsubstringwithoutrepeatingcharacters"],"traits":[{"interfaceLanguage":"swift"}]}],"references":{"doc://LeetCodeDocs/documentation/LeetCodeDocs/LongestSubstringWithoutRepeatingCharacters":{"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"LongestSubstringWithoutRepeatingCharacters"}],"navigatorTitle":[{"kind":"identifier","text
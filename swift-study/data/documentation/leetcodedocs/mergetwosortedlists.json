{"schemaVersion":{"major":0,"minor":3,"patch":0},"hierarchy":{"paths":[["doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs"]]},"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/MergeTwoSortedLists"},"topicSections":[{"anchor":"Classes","title":"Classes","generated":true,"identifiers":["doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/MergeTwoSortedLists\/ListNode"]}],"kind":"symbol","metadata":{"role":"symbol","symbolKind":"struct","navigatorTitle":[{"kind":"identifier","text":"MergeTwoSortedLists"}],"externalID":"s:12LeetCodeDocs19MergeTwoSortedListsV","modules":[{"name":"LeetCodeDocs"}],"title":"MergeTwoSortedLists","roleHeading":"Structure","fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"text":"MergeTwoSortedLists","kind":"identifier"}],"color":{"standardColorIdentifier":"green"}},"abstract":[{"text":"LeetCode #21 • Easy • Linked Lists","type":"text"}],"seeAlsoSections":[{"title":"Linked Lists","anchor":"Linked-Lists","identifiers":["doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/AddTwoNumbers"],"generated":true}],"variants":[{"paths":["\/documentation\/leetcodedocs\/mergetwosortedlists"],"traits":[{"interfaceLanguage":"swift"}]}],"sections":[],"primaryContentSections":[{"declarations":[{"languages":["swift"],"tokens":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"MergeTwoSortedLists","kind":"identifier"}],"platforms":["macOS"]}],"kind":"declarations"},{"content":[{"text":"Overview","type":"heading","level":2,"anchor":"overview"},{"inlineContent":[{"type":"text","text":"Merge Two Sorted Lists"}],"type":"paragraph"},{"type":"heading","anchor":"Intuition","text":"Intuition","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"We use a ‘Dummy Head’ node to simplify edge cases. We iterate through both lists with pointers "},{"type":"codeVoice","code":"l1"},{"type":"text","text":" and "},{"type":"codeVoice","code":"l2"},{"type":"text","text":", attaching the smaller node to our "},{"type":"codeVoice","code":"curr"},{"type":"text","text":" pointer. Once one list is exhausted, we attach the remainder of the other list immediately."}]},{"type":"thematicBreak"},{"type":"heading","level":2,"anchor":"Complexity-Analysis","text":"Complexity Analysis"},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Time Complexity"}]},{"type":"text","text":": "},{"type":"codeVoice","code":"O(N + M)"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"text":"Space Complexity","type":"text"}],"type":"strong"},{"text":": ","type":"text"},{"type":"codeVoice","code":"O(1)"}]}]}]},{"type":"thematicBreak"},{"type":"heading","level":2,"anchor":"Solution","text":"Solution"},{"code":["\/\/ Definition for singly-linked list.","public class ListNode {","    public var val: Int","    public var next: ListNode?","    public init() { self.val = 0; self.next = nil; }","    public init(_ val: Int) { self.val = val; self.next = nil; }","    public init(_ val: Int, _ next: ListNode?) { self.val = val; self.next = next; }","}","","class Solution {","    func mergeTwoLists(_ list1: ListNode?, _ list2: ListNode?) -> ListNode? {","        let dummy = ListNode(-1)","        var curr = dummy","        var l1 = list1","        var l2 = list2","","        while let n1 = l1, let n2 = l2 {","            if n1.val < n2.val {","                curr.next = n1","                l1 = n1.next","            } else {","                curr.next = n2","                l2 = n2.next","            }","            curr = curr.next!","        }","","        curr.next = l1 ?? l2","        return dummy.next","    }","}"],"type":"codeListing","syntax":"swift"}],"kind":"content"}],"references":{"doc://LeetCodeDocs/documentation/LeetCodeDocs":{"title":"LeetCodeDocs","type":"topic","identifier":"doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs","url":"\/documenta
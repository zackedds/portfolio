{"abstract":[{"text":"LeetCode #66 • Easy • Arrays & Math","type":"text"}],"primaryContentSections":[{"declarations":[{"platforms":["macOS"],"languages":["swift"],"tokens":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"PlusOne","kind":"identifier"}]}],"kind":"declarations"},{"content":[{"level":2,"type":"heading","text":"Overview","anchor":"overview"},{"inlineContent":[{"text":"Plus One","type":"text"}],"type":"paragraph"},{"level":2,"text":"Intuition","type":"heading","anchor":"Intuition"},{"inlineContent":[{"text":"We iterate backwards. If a digit is less than 9, we simply increment it and return the array immediately (Early Exit). If the digit is 9, we set it to 0 and continue to the next digit. If we finish the loop without returning, it means we had all 9s (e.g., 99 -> 00), so we insert 1 at the start.","type":"text"}],"type":"paragraph"},{"type":"thematicBreak"},{"level":2,"text":"Complexity Analysis","type":"heading","anchor":"Complexity-Analysis"},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Time Complexity"}]},{"type":"text","text":": "},{"code":"O(N)","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Space Complexity"}]},{"text":": ","type":"text"},{"code":"O(N)","type":"codeVoice"}]}]}]},{"type":"thematicBreak"},{"level":2,"type":"heading","anchor":"Solution","text":"Solution"},{"syntax":"swift","code":["class Solution {","    func plusOne(_ digits: [Int]) -> [Int] {","        var result = digits","        ","        for i in (0..<result.count).reversed() {","            if result[i] < 9 {","                result[i] += 1","                return result","            }","            \/\/ If it was 9, it becomes 0 and we carry over to the next loop iteration","            result[i] = 0","        }","        ","        \/\/ Edge case: All digits were 9 (e.g., [9,9] -> [0,0])","        result.insert(1, at: 0)","        return result","    }","}"],"type":"codeListing"}],"kind":"content"}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/PlusOne"},"sections":[],"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/leetcodedocs\/plusone"]}],"hierarchy":{"paths":[["doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs"]]},"metadata":{"symbolKind":"struct","modules":[{"name":"LeetCodeDocs"}],"fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"PlusOne"}],"title":"PlusOne","externalID":"s:12LeetCodeDocs7PlusOneV","roleHeading":"Structure","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"PlusOne"}],"color":{"standardColorIdentifier":"green"}},"schemaVersion":{"major":0,"minor":3,"patch":0},"references":{"doc://LeetCodeDocs/documentation/LeetCodeDocs/PlusOne":{"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"PlusOne"}],"abstract":[{"text":"LeetCode #66 • Easy • Arrays & Math","type":"text"}],"kind":"symbol","type":"topic","url":"\/documentation\/leetcodedocs\/plusone","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"PlusOne"}],"identifier":"doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs\/PlusOne","title":"PlusOne"},"doc://LeetCodeDocs/documentation/LeetCodeDocs":{"title":"LeetCodeDocs","type":"topic","identifier":"doc:\/\/LeetCodeDocs\/documentation\/LeetCodeDocs","url":"\/documentation\/leetcodedocs","role":"collection","kind":"symbol","abstract":[{"type":"text","text":"My collection of Swift solutions for LeetCode problems, focusing on optimization, time complexity, and language features."}]}}}